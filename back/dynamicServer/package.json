{
  "name": "learniba-dynamic-server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "src/app.js",
  "directories": {
    "public": "public",
    "src": "src"
  },
  "scripts": {
    "start": "node_modules/.bin/nodemon ./src/server.js",
    "start2": "node_modules/.bin/webpack --watch | nodemon public/app/app.js",
    "build": "node_modules/.bin/webpack --mode=production --node-env=production",
    "build:dev": "node_modules/.bin/webpack --mode=development",
    "build:prod": "node_modules/.bin/webpack --mode=production --node-env=production",
    "watch": "node_modules/.bin/webpack --watch",
    "serve": "node_modules/.bin/webpack serve",
    "test": "NODE_ENV=test  node_modules/.bin/tap",
    "test:coverage": "node_modules/.bin/test --coverage-report=lcov  ",
    "test:unit": "node_modules/.bin/mocha --recursive test/middleware test/models test/routes",
    "test:integration": "node_modules/.bin/mocha --recursive test/integration"
  },
